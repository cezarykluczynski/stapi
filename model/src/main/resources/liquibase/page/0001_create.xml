<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
				   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
	<changeSet id="0001_create" author="cezarykluczynski">
		<createTable tableName="page">
			<column name="id" type="number(38, 0)">
				<constraints unique="true" nullable="false"/>
			</column>
			<column name="page_id" type="number(38, 0)">
				<constraints unique="true" nullable="false"/>
			</column>
			<column name="media_wiki_source" type="varchar(32)">
				<constraints nullable="false"/>
			</column>
			<column name="title" type="varchar(128)">
				<constraints nullable="false"/>
			</column>
		</createTable>
		<createSequence incrementBy="1" sequenceName="page_sequence"/>
	</changeSet>
</databaseChangeLog>
