<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
	<changeSet id="0044_create_spacecraft_type" author="cezarykluczynski">
		<createTable schemaName="stapi" tableName="spacecraft_type">
			<column name="id" type="bigint">
				<constraints unique="true" nullable="false"/>
			</column>
			<column name="page_id" type="bigint">
				<constraints nullable="false"/>
			</column>
			<column name="u_id" type="varchar(14)">
				<constraints nullable="false"/>
			</column>
			<column name="name" type="varchar(32)">
				<constraints nullable="false"/>
			</column>
		</createTable>
		<createSequence incrementBy="1" sequenceName="spacecraft_type_sequence" schemaName="stapi"/>
		<addForeignKeyConstraint baseColumnNames="page_id"
				baseTableName="spacecraft_type"
				baseTableSchemaName="stapi"
				constraintName="spacecraft_type_page_fkey"
				referencedColumnNames="id"
				referencedTableName="page"
				referencedTableSchemaName="stapi"/>
	</changeSet>
</databaseChangeLog>
