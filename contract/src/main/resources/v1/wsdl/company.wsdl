<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions
		name="Company"
		targetNamespace="http://stapi.co/api/v1/soap/company"
		xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
		xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
		xmlns:stapi="http://stapi.co/api/v1/soap/company"
		xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<wsdl:types>
		<xs:schema targetNamespace="http://stapi.co/api/v1/soap/company">
			<xs:include schemaLocation="company.xsd"/>
		</xs:schema>
	</wsdl:types>
	<wsdl:message name="CompanyRequest">
		<wsdl:part name="request" element="stapi:CompanyRequest"/>
	</wsdl:message>
	<wsdl:message name="CompanyResponse">
		<wsdl:part name="response" element="stapi:CompanyResponse"/>
	</wsdl:message>
	<wsdl:portType name="CompanyPortType">
		<wsdl:operation name="getCompanies">
			<wsdl:input name="getCompanyRequest" message="stapi:CompanyRequest"/>
			<wsdl:output name="getCompanyResponse" message="stapi:CompanyResponse"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="CompanyBinding" type="stapi:CompanyPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="getCompanies">
			<wsdl:input name="getCompanyRequest">
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="getCompanyResponse">
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="CompanyService">
		<wsdl:port name="CompanyPortType" binding="stapi:CompanyBinding">
			<soap:address location="http://stapi.co/api/v1/soap/company"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
