<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		elementFormDefault="qualified"
		targetNamespace="http://stapi.co/api/v1/soap/contentLanguage">
	<xs:complexType name="ContentLanguage">
		<xs:annotation>
			<xs:documentation>Rating of video release, etc.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="uid" type="xs:string">
				<xs:annotation>
					<xs:documentation>Language unique ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:string">
				<xs:annotation>
					<xs:documentation>Language name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="iso6391Code" type="xs:string">
				<xs:annotation>
					<xs:documentation>ISO 639-1 code</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
