							</xs:annotation>
						</xs:element>
						<xs:element name="birthName" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Performer birth name</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="gender" type="common:GenderEnum" minOccurs="0">
							<xs:annotation>
