<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions
		name="Character"
		targetNamespace="http://stapi.co/api/v1/soap/character"
		xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
		xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
		xmlns:stapi="http://stapi.co/api/v1/soap/character"
		xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<wsdl:types>
		<xs:schema targetNamespace="http://stapi.co/api/v1/soap/character">
			<xs:include schemaLocation="character.xsd"/>
		</xs:schema>
	</wsdl:types>
	<wsdl:message name="CharacterRequest">
		<wsdl:part name="request" element="stapi:CharacterRequest"/>
	</wsdl:message>
	<wsdl:message name="CharacterResponse">
		<wsdl:part name="response" element="stapi:CharacterResponse"/>
	</wsdl:message>
	<wsdl:portType name="CharacterPortType">
		<wsdl:operation name="getCharacters">
			<wsdl:input name="getCharactersRequest" message="stapi:CharacterRequest"/>
			<wsdl:output name="getCharactersResponse" message="stapi:CharacterResponse"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="CharacterBinding" type="stapi:CharacterPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="getCharacters">
			<wsdl:input name="getCharactersRequest">
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="getCharactersResponse">
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="CharacterService">
		<wsdl:port name="CharacterPortType" binding="stapi:CharacterBinding">
			<soap:address location="http://stapi.co/api/v1/soap/character"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
