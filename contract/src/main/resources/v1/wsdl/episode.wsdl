<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions
		name="Episode"
		targetNamespace="http://stapi.co/api/v1/soap/episode"
		xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
		xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
		xmlns:stapi="http://stapi.co/api/v1/soap/episode"
		xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<wsdl:types>
		<xs:schema targetNamespace="http://stapi.co/api/v1/soap/episode">
			<xs:include schemaLocation="episode.xsd"/>
		</xs:schema>
	</wsdl:types>
	<wsdl:message name="EpisodeRequest">
		<wsdl:part name="request" element="stapi:EpisodeRequest"/>
	</wsdl:message>
	<wsdl:message name="EpisodeResponse">
		<wsdl:part name="response" element="stapi:EpisodeResponse"/>
	</wsdl:message>
	<wsdl:portType name="EpisodePortType">
		<wsdl:operation name="getEpisodes">
			<wsdl:input name="getEpisodesRequest" message="stapi:EpisodeRequest"/>
			<wsdl:output name="getEpisodesResponse" message="stapi:EpisodeResponse"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="EpisodeBinding" type="stapi:EpisodePortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="getEpisodes">
			<wsdl:input name="getEpisodesRequest">
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output name="getEpisodesResponse">
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="EpisodeService">
		<wsdl:port name="EpisodePortType" binding="stapi:EpisodeBinding">
			<soap:address location="http://stapi.co/api/v1/soap/episode"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
