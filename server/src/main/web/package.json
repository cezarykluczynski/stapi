{
	"name": "stapi",
	"version": "0.0.14-SNAPSHOT",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "grunt clean && ng build && grunt copy",
		"test": "ng test --sourcemaps=false",
		"test-once": "ng test --watch=false",
		"test-cov": "ng test --sourcemaps=false --code-coverage --reporters=coverage-istanbul,progress,kjhtml",
		"test-once-cov": "ng test --watch=false --sourcemaps=false --code-coverage --reporters=coverage-istanbul,progress,kjhtml",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"build-prod": "grunt clean && ng build --target=production --environment=prod --build-optimizer=false && grunt copy",
		"install-build": "npm install && npm run build-prod"
	},
	"private": true,
