{
	"name": "stapi",
	"version": "0.1.4-SNAPSHOT",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "grunt clean && ng build && grunt copy",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"test-once": "ng test --watch=false",
		"test-cov": "ng test  --code-coverage --karma-config=./karma.conf.js",
		"test-once-cov": "ng test --watch=false --code-coverage --karma-config=./karma.conf.js",
		"lint": "ng lint",
		"build-prod": "grunt clean && ng build --configuration=production --build-optimizer=false && grunt copy",
		"install-build": "npm ci && npm run build-prod"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "16.0.3",
		"@angular/common": "16.0.3",
		"@angular/compiler": "16.0.3",
		"@angular/core": "16.0.3",
		"@angular/forms": "16.0.3",
		"@angular/platform-browser": "16.0.3",
		"@angular/platform-browser-dynamic": "16.0.3",
		"@angular/router": "16.0.3",
		"@types/jquery": "3.5.16",
		"another-rest-client": "0.7.0",
		"bootstrap": "5.3.0",
		"bootswatch": "5.3.0",
		"jquery": "3.7.0",
		"rxjs": "7.8.1",
		"tslib": "2.5.2",
		"zone.js": "0.13.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "16.0.3",
		"@angular-eslint/builder": "16.0.3",
		"@angular-eslint/eslint-plugin": "16.0.3",
		"@angular-eslint/eslint-plugin-template": "16.0.3",
		"@angular-eslint/schematics": "16.0.3",
		"@angular-eslint/template-parser": "16.0.3",
		"@angular/cli": "16.0.3",
		"@angular/compiler-cli": "16.0.3",
		"@types/jasmine": "4.3.2",
		"@typescript-eslint/eslint-plugin": "5.59.8",
		"@typescript-eslint/parser": "5.59.8",
		"eslint": "8.41.0",
		"grunt": "1.6.1",
		"grunt-contrib-clean": "2.0.1",
		"grunt-contrib-copy": "1.0.0",
		"grunt-exec": "3.0.0",
		"jasmine-core": "4.6.0",
		"karma": "6.4.2",
		"karma-chrome-launcher": "3.2.0",
		"karma-coverage": "2.2.0",
		"karma-jasmine": "5.1.0",
		"karma-jasmine-html-reporter": "2.0.0",
		"karma-junit-reporter": "2.0.1",
		"properties-parser": "0.6.0",
		"typescript": "5.0.4"
	}
}
